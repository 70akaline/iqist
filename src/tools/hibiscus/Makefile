default: all

all: build-script build-toolbox
clean: clean-script clean-toolbox

# targets for script
build-script:
	cd ./script; pwd; make

clean-script:
	cd ./script; pwd; make clean

# targets for toolbox
build-toolbox:
	cd ./toolbox; pwd; make

clean-toolbox:
	cd ./toolbox; pwd; make clean
