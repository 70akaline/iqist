default: all

all: build-entropy1 build-entropy2 build-sac build-script build-swing build-toolbox
clean: clean-entropy1 clean-entropy2 clean-sac clean-script clean-swing clean-toolbox

# targets for entropy1
build-entropy1:
	cd ./entropy1; pwd; make

clean-entropy1:
	cd ./entropy1; pwd; make clean

# targets for entropy2
build-entropy2:
	cd ./entropy2; pwd; make

clean-entropy2:
	cd ./entropy2; pwd; make clean

# targets for sac
build-sac:
	cd ./sac; pwd; make

clean-sac:
	cd ./sac; pwd; make clean

# targets for script
build-script:
	cd ./script; pwd; make

clean-script:
	cd ./script; pwd; make clean

# targets for swing
build-swing:
	cd ./swing; pwd; make

clean-swing:
	cd ./swing; pwd; make clean

# targets for toolbox
build-toolbox:
	cd ./toolbox; pwd; make

clean-toolbox:
	cd ./toolbox; pwd; make clean
